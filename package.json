{
  "name": "feignjs",
  "version": "0.0.2",
  "description": "feign for js",
  "main": "feign.js",
  "scripts": {
    "test": "node_modules/.bin/nodeunit tests",
    "test-cov": "istanbul cover testsuite.js && cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "bower": "browserify feign.js -o dist/feign.js --s feign --no-bundle-external && uglifyjs dist/feign.js -o dist/feign.min.js",
    "prepublish": "npm run-script bower"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warmuuh/feignjs.git"
  },
  "keywords": [
    "feign",
    "rest",
    "descriptive",
    "client"
  ],
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "args-js": "global:Args",
    "lodash": "global:_",
    "uri-templates": "global:UriTemplate"
  },
  "author": "Peter Mucha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/warmuuh/feignjs/issues"
  },
  "homepage": "https://github.com/warmuuh/feignjs#readme",
  "dependencies": {
    "args-js": "^0.10.6",
    "lodash": "^3.9.3",
    "uri-templates": "^0.1.7"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "browserify-shim": "^3.8.8",
    "codecov.io": "^0.1.2",
    "nodeunit": "^0.9.1"
  }
}
